{
  "name": "moro/platform-cms",
  "description": "Platform - content management system",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Andrey Morozkin",
      "email": "andrey.dmitrievich@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {"Moro\\Platform\\": "sources/"},
    "files": [
      "sources/functions.php"
    ],
    "classmap": [
      "sources/application.php"
    ]
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "fengyuanchen/cropperp",
        "version": "0.11.1",
        "source": {
          "url": "git@github.com:fengyuanchen/cropper.git",
          "type": "git",
          "reference": "tags/v0.11.1"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mustache/mustache-js",
        "version": "2.0.0",
        "source": {
          "url": "git@github.com:janl/mustache.js.git",
          "type": "git",
          "reference": "tags/v2.0.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "jackmoore/autosize",
        "version": "3.0.4",
        "source": {
          "url": "git@github.com:jackmoore/autosize.git",
          "type": "git",
          "reference": "tags/3.0.4"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5",
    "silex/silex": "~1.3",
    "doctrine/dbal": "~2.5",
    "symfony/console": "~2.6",
    "symfony/security": "~2.6",
    "symfony/form": "~2.6",
    "symfony/validator": "~2.6",
    "symfony/config": "~2.6",
    "symfony/translation": "~2.6",
    "symfony/security-csrf": "~2.6",
    "symfony/twig-bridge": "~2.6",
    "knplabs/console-service-provider": "~1.0",
    "knplabs/knp-menu": "~2.0",
    "monolog/monolog": "~1.8",
    "twig/twig": "~1.18",
    "moro/team-migrations-silex": "~1.4",
    "robloach/component-installer": "~0.2",
    "fengyuanchen/cropperp": "0.11.1",
    "components/jquery": "~1.9",
    "components/bootstrap": "~3.3",
    "saxulum/saxulum-bootstrap-provider": "~1.0",
    "michelf/php-markdown": "~1.5",
    "aptoma/twig-markdown": "~1.2",
    "kartik-v/bootstrap-fileinput": "~4.1",
    "imagine/imagine": "~0.6.2",
    "select2/select2": "~4.0",
    "mustache/mustache-js": "~2.0",
    "jackmoore/autosize": "~3.0"
  },
  "config": {
    "component-dir": "http/assets",
    "component-baseurl": "/assets",
    "component-scriptFilters-disabled": {
      "\\Assetic\\Filter\\GoogleClosure\\CompilerApiFilter": []
    }
  },
  "extra": {
    "component": {
      "name": "platform",
      "scripts": [
        "resources/scripts/bootstrap.js",
        "resources/scripts/flash-message.js"
      ],
      "styles": [
        "resources/styles/bootstrap.css"
      ],
      "files": [
        "resources/scripts/*.js",
        "resources/styles/*.css",
        "resources/images/*.jpg",
        "resources/images/*.png",
        "resources/images/*.gif",
        "resources/*.html"
      ],
      "components/bootstrap": {
        "scripts": [
          "js/bootstrap.js"
        ],
        "styles": [
          "css/bootstrap.css",
          "css/bootstrap-theme.css"
        ],
        "files": [
          "fonts/*"
        ],
        "shim": {
          "deps": ["jquery"]
        }
      },
      "kartik-v/bootstrap-fileinput": {
        "name": "bootstrap-fileinput",
        "scripts": [
          "js/fileinput.js",
          "js/fileinput_locale_ru.js"
        ],
        "styles": [
          "css/fileinput.css"
        ],
        "files": [
          "img/*"
        ],
        "shim": {
          "deps": ["jquery", "bootstrap"]
        }
      },
      "fengyuanchen/cropperp": {
        "name": "bootstrap-cropper",
        "scripts": [
          "dist/cropper.js"
        ],
        "styles": [
          "dist/cropper.css"
        ],
        "shim": {
          "deps": ["jquery", "bootstrap"]
        }
      },
      "select2/select2": {
        "scripts": [
          "dist/js/select2.full.js"
        ],
        "styles": [
          "dist/css/select2.css"
        ],
        "files": [
          "dist/js/i18n/ru.js"
        ],
        "shim": {
          "deps": ["jquery", "bootstrap"]
        }
      },
      "mustache/mustache-js": {
        "name": "mustache",
        "scripts": [
          "mustache.js"
        ]
      },
      "jackmoore/autosize": {
        "name": "textarea_autosize",
        "scripts": [
          "dest/autosize.js"
        ]
      }
    }
  }
}
